@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>TESODEV</title>
    <link rel="stylesheet" href="~/admin panel/admin/plugins/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/admin panel/admin/dist/css/adminlte.min.css">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>
        </nav>


        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <a href="@Url.Action("Index", "Home")" class="brand-link">
                <img src="~/admin panel/admin/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">TESODEV</span>
            </a>

            <div class="sidebar">
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/Images/User.png" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">
                            @if (Session["Name"] == null)
                            {

                            }
                            else
                            {@Session["Name"].ToString()

                        }
                        </a>
                    </div>
                </div>
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item">
                            <a href="@Url.Action("Index", "Home")" class="nav-link">

                                <p>
                                    Home Page

                                </p>
                            </a>

                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("Index", "Order")" class="nav-link">

                                <p>
                                    My Orders

                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">

                                <p>
                                    Admin Page
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("Index", "Admin")" class="nav-link">

                                        <p>Customers</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ManegeOrder", "Admin")" class="nav-link">

                                        <p>Orders</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("Product", "Admin")" class="nav-link">

                                        <p>Product</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-header">Status</li>
                        <li class="nav-item">
                            <a href=" @Url.Action("LoginCustomersforMYOrders", "Login")" class="nav-link">

                                <p>
                                    Log İn

                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href=" @Url.Action("Logout", "Home")" class="nav-link">

                                <p>
                                    Log Out

                                </p>
                            </a>
                        </li>


                    </ul>
                </nav>
            </div>
        </aside>
        <div class="content-wrapper">
            <div class="content">
                @RenderBody()
            </div>

        </div>


    </div>
    <script src="~/admin panel/admin/plugins/jquery/jquery.min.js"></script>
    <script src="~/admin panel/admin/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/admin panel/admin/dist/js/adminlte.js"></script>
    <script src="~/admin panel/admin/plugins/chart.js/Chart.min.js"></script>
    <script src="~/admin panel/admin/dist/js/demo.js"></script>
    <script src="~/admin panel/admin/dist/js/pages/dashboard3.js"></script>
</body>
</html>
